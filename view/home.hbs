<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css">
  <link rel="stylesheet" href="/static/css/style.css">
  <title>Hello, world!</title>
</head>

<body>

  {{!-- Navebar section --}}
  {{> navbar data=details}}

  {{!-- Slider section --}}
  {{> slider slide=slide}}

  {{!-- first banner section --}}

  <section class="banner_section py-4  background1">
    <div class="container text-center">
      <div class="row">
        <div class="col-md-12">
          <h1>Start Learning with technical solution</h1>
          <p>Start Learning with technical solution Start Learning with technical solution Start Learning with technical
            solution </p>
          <button class="btn btn-primary">Start learning</button>
        </div>
      </div>
    </div>

  </section>

  {{!-- service section --}}

  <section class="service_section" id="ser_section">
    <h1 class="text-center text-warning py-2">Our services</h1>
    <div class="container">
      <div class="row mt-5 py-3">

        {{#each services}}
        <div class="col-md-4 py-4">
          <div class="card ">
            <div class="container text-center">
              <i class="{{this.icon}} service_icon"></i>
            </div>
            <div class="card-body ">
              <h5 class="card-title">{{this.title}}</h5>
              <p>{{this.description}}
              <p>
                <a href="{{this.link}}">{{this.linktext}}</a>
                <a href="#" class="ml-3">support</a>
            </div>
          </div>
        </div>
        {{/each}}

      </div>
    </div>
  </section>

  {{!-- Second banner section --}}


  <section class="banner2_section  border-bottom py-2 mb-2">
    <div class="container text-center py-2">
      <div class="row">

        {{!-- left_side --}}
        <div class="col-md-6" style="height: 320px; margin-top: 40px;">
          <h1>{{bannerdata.firstbabnner.bannertitle}}</h1>
          <p>{{bannerdata.firstbabnner.discription}}</p>
          <button class="btn btn-primary">Check here</button>
        </div>
        {{!-- right_side --}}
        <div class="col-md-6">
          <img class="banner1_img" src={{bannerdata.firstbabnner.bannerurl}} alt="...">
        </div>

      </div>
    </div>

  </section>

  {{!-- Third banner_section --}}

  <section class="banner2_section  border-bottom py-2 mb-2">
    <div class="container text-center py-2">
      <div class="row">


        {{!-- left_side --}}

        <div class="col-md-6">
          <img class="banner2_img" src={{bannerdata.secondbanner.bannerurl}} alt="...">
        </div>

        {{!-- right_side --}}
        <div class="col-md-6" style="height: 320px; margin-top: 40px;">
          <h1>{{bannerdata.secondbanner.bannertitle}}</h1>
          <p>{{bannerdata.secondbanner.discription}}</p>
          <button class="btn btn-primary">Check here</button>
        </div>


      </div>
    </div>

  </section>



  {{!-- constect section --}}

  <section id="contect_us_section" class=" py-5  background1">
    <div class="container ">
      <h3 class="text-center text-muted">Contect Us</h3>
      <div class="row mt-5">
        <div class="col-md-8 m-auto">

          <form action="/user-contect" method="post">
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">your email address</label>
              <input type="email" class="form-control" name="email" id="exampleInputEmail1"
                aria-describedby="emailHelp">
              <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
            </div>

            <div class="mb-3">
              <label for="exampleInputPassword1" class="form-label">Phone no</label>
              <input type="number" name="number" class="form-control" id="exampleInputPassword1">
            </div>

            <div class="form-floating mt-4">
              <textarea name="query" style="height: 250px;" class="form-control" placeholder="enter your query"
                id="floatingTextarea"></textarea>
              <label for="floatingTextarea">enter your query</label>
            </div>

            <div class="container text-center mt-4 ">
              <button type="submit" class="btn btn-primary text-warning"> Submit</button>
            </div>
          </form>
        </div>
      </div>
    </div>

  </section>

  {{!-- About_Us --}}

  <section class="about_section  py-4" id="about">
    <div class="container text-center">
      <div class="row">
        <div class="col-md-12">
          <h1>{{aboutdata.title}}</h1>
          <p> {{aboutdata.about_description}} </p>
          <button class="btn btn-success  btn-lg mt-4" style="width:100px">Contect</button>
        </div>

      </div>
    </div>
  </section>

  {{!-- footer_section --}}

  {{> footer}}

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
    crossorigin="anonymous"></script>


</body>

</html>